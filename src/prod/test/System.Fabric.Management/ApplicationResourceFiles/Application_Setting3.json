{
  "name":"myapp",
  "properties":{
    "description":"Hello world seabreeze",
    "services":[
      {
        "name":"myBackendService",
        "properties":{
          "description":"Seabreeze backend service.",
          "codePackages":[
            {
              "name":"backend",
              "image":"compose-test-backend:v2",
              "imageRegistryCredential":{
                "server":"sftestcontainerreg.azurecr.io",
                "username":"sftestcontainerreg",
                "password":"password"
              },
              "endpoints":[
                {
                  "name":"publicListener",
                  "port":"3031"
                }
              ],
              "environmentVariables":[
                {
                  "name":"backendpoint",
                  "value":"http://+:3031/"
                }
              ],
              "settings": [
                {
                  "name": "Hosting/Timeout",
                  "value": "300"
                },
				        {
                  "name": "name",
                  "value": "[encrypted()]"
                },
                {
                  "name": "secret",
                  "value": "[reference('secrets/secRet1_-/values/1.0-alpha_0')]"
                }
              ],
              "resources":{
                "requests":{
                  "cpu":"1",
                  "memoryInGB":"1"
                }
              }
            }
          ],
          "replicaCount":"2",
          "autoScalingPolicies": [
            {
              "name": "MyAutoScaleRule",
              "trigger":
              {
                "kind": "AverageLoad",
                "metric":
                {
                  "kind": "Resource",
                  "name": "cpu"
                },
                "lowerLoadThreshold": "0.3",
                "upperLoadThreshold": "1.5",
                "scaleIntervalInSeconds": "3600"
              },
              "mechanism":
              {
                "kind": "AddRemoveReplica",
                "minCount": "1",
                "maxCount": "4",
                "scaleIncrement": "1"
              }
            }
          ],
          "networkRefs":[
            {
              "name":"mynetwork"
            }
          ]
        }
      }
    ]
  }
}